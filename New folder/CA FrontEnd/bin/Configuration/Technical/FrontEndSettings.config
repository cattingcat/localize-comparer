<?xml version="1.0" encoding="utf-8" ?>
<FrontEndSettings>
  <add key="EnableUiLogging" value="false" />  <!-- Логгирование в консоль браузера -->
  <add key="EnableUiLocalizeErrorsReplacing" value="false" /> <!-- Заменять ли ошибки локализации на дружественные сообщения -->
  <add key="EnableUiAjaxTracing" value="false" /> <!-- Трассировка AJAX-запросов в нижней части экрана -->
  <add key="AjaxTracingCount" value="30" /> <!-- Количество отображаемых AJAX-запросов при трассировке -->
  <add key="SendRequestIdFromUi" value="true" />  <!-- Включать ли заголовок с идентификатором запроса в запрос HTTP -->
  <!-- Коды ошибок, которые обрабатываются для все AJAX-запросов одинаково: -->
  <add key="CommonErrorCodes" value="UnknownError,UserNotFound,NotMainUser,AccessDenied,Failure,NotSupportedCompanyCategory,NotSupportedCompanyCategorySecurity,UnexpectedSecurityException" />
  <add key="TokenErrorCodes" value="Unauthorized,Forbidden" />  <!-- Коды ошибок, после которых идем за новым токеном: -->
  <add key="UiFileNameEllipsisDefault" value="30" />  <!-- Максимальная длина имени файла при отображения на UI (если длина имени файла превышает заданную то происходит эллипсис) -->
  <add key="UiFileNameEllipsisClientRegistration" value="12" /> <!-- Максимальная длина имени файла при отображения на UI при регистрации клиента, (если длина имени файла превышает заданную то происходит эллипсис) -->
  <add key="UiKasperskyDefaultLogoUrl" value="/Img/logo/kaspersky.png" /> <!-- UI HTTP Url к логотипу отображаемому по умолчанию -->
  <add key="AccessTokenUiExpiration" value="12" />  <!-- За сколько секунд до истечения срока жизни токена UI запросит новый -->
  <add key="FileNotFoundErrorRedirectUrl" value="/error/fileNotFound"/> <!--Путь до страницы fileNotFound в случае, если файл не найден--> 
  <add key="ForbiddenErrorRedirectUrl" value="/error/forbidden"/> <!--Путь до страницы forbidden в случае анонимной загрузки--> 
  <add key="EnableGoogleAnalytics" value="true" />  <!-- Использовать Google Analytics -->
  <add key="GoogleAnalyticsAccountId" value="UA-79650446-1" /> <!-- Google Analytics Account Id -->
  <add key="EnableGoogleTagManager" value="false" /><!-- Использовать Google Tag Manager -->
  <add key="GoogleTagManagerAccountId" value="GTM-MPK6ZG" /><!-- Google Tag Manager Account Id -->
  <add key="GoogleTagManagerWebSite" value="https://companyaccount.kaspersky.com" /><!-- Google Tag Manager Web Site -->
</FrontEndSettings>