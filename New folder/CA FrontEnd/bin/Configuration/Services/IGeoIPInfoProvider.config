<?xml version="1.0" encoding="utf-8" ?>
<configuration>
  <system.serviceModel>
    <bindings>			
      <netNamedPipeBinding>
        <binding name="GeoIPInfoProviderClientBinding_StageStand" sendTimeout="00:01:00"/>
      </netNamedPipeBinding>
    </bindings>
    <client>
      <endpoint address="net.pipe://localhost/IPInfoService/1.2/IPInfoService.svc"
          binding="netNamedPipeBinding" bindingConfiguration="GeoIPInfoProviderClientBinding_StageStand"
          contract="IPInfoService.IGeoIPInfoProvider"
          behaviorConfiguration="logEndPoint"
          name="IGeoIPInfoProviderClientEndpoint_StageStand" >
			<identity>
				<userPrincipalName value=".\svc_IPInfo_Local" />
			</identity>
      </endpoint>
    </client>
    <behaviors>
      <endpointBehaviors>
        <behavior name="logEndPoint">
          <!--<logBehavior />-->
           <!--  <perfLogBehavior/> -->
        </behavior>
      </endpointBehaviors>
    </behaviors>
    <extensions>
      <behaviorExtensions>
        <add name="logBehavior" 
             type="KasperskyLab.CompanyAccount.WcfInfrastructure.Logging.LoggerBehaviorExtensionElement, KasperskyLab.CompanyAccount.WcfInfrastructure, Version=1.0.0.0, Culture=neutral" />
		<add name="perfLogBehavior"
             type="KasperskyLab.CompanyAccount.WcfInfrastructure.Logging.PerfomaceLoggerExtensionElement, KasperskyLab.CompanyAccount.WcfInfrastructure, Version=1.0.0.0, Culture=neutral" />
      </behaviorExtensions>
    </extensions>
  </system.serviceModel>
</configuration>