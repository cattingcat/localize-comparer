<parameters>

  <!--Logging-->
  <parameter name="Logs folder"
    description="Folder for log files"
    defaultValue="Logs\"
    tags="">
    <parameterEntry
      kind="XmlFile"
      scope="\\Configuration\\Logging\\Logging.config$"
      match="//appender[@name='MainLog_FileAppender']/file/@value" />
  </parameter>

  <parameter name="PerformanceLog folder"
    description="Folder for log files"
    defaultValue="Logs\"
    tags="">
