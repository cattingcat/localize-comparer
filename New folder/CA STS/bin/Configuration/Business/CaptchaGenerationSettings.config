<?xml version="1.0" encoding="utf-8" ?>
<CaptchaGenerationSettings>
  <add key="CaptchaPattern" value="$[abdefknrstyz23456789]*4" />  <!--Выражение для капчи, включающее в себя доступные символы и количество символов-->
  <add key="CaptchaTime" value="600" />                           <!--Время действия капчи-->
</CaptchaGenerationSettings>
